{
    "name": "Deploy",
    "description": "Deploy",
    "keywords": ["Deploy"],
    "env": {
        "GLOBAL_USER": {
            "description": "Username",
            "value": "admin"
        },        
        "GLOBAL_PASSWORD": {
            "description": "Password, double as Aria2 RPC token",
            "value": "password"
        },
        "GLOBAL_LANGUAGE": {
            "description": "Set language of portal page & filebrowser.(en or chs)",
            "value": "en"
        },
        "GLOBAL_PORTAL_PATH": {
            "description": "Portal page & base URL for all web services",
            "value": "/portal"
